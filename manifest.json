{
    "manifest_version": 2,

    "name": "spandex",
    "description": "A text expander extension for Chromium.",
    "version": "1.0",

    "permissions": [
        "<all_urls>", "storage"
    ],
    "content_scripts": [
        {
            "matches": ["<all_urls>"],
            "js": ["jquery.js", "spandex.js"]
        }
    ],
    "options_page": "options.html"
}
